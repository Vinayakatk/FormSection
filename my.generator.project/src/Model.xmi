<?xml version="1.0" encoding="ASCII"?>
<metamodel:Model
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:metamodel="http://www.example.org/metamodel"
    xsi:schemaLocation="http://www.example.org/metamodel metamodel/metamodel.ecore">
  <types xsi:type="metamodel:Datatype"
      name="String"/>
  <types xsi:type="metamodel:Datatype"
      name="Integer"/>
  <types xsi:type="metamodel:Entity"
      name="Person">
    <features
        name="name"
        type="//@types.0"/>
    <features
        name="age"
        type="//@types.1"/>
    <features
        name="address"/>
  </types>
</metamodel:Model>
